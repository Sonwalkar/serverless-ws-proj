{
  "compilerOptions": {
    "target": "es2022", 
    "lib": ["es2022"],
    "module": "commonjs",       
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "lib", 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "removeComments": true,
  }
}
